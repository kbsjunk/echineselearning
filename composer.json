{
	"name": "kitbs/echineselearning",
	"type": "platform-extension",
	"description": "eChineseLearning Calendar",
	"authors": [
		{
			"name": "KitBS",
			"email": "kitbs@kitbs.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.1.*",
		"illuminate/support": "4.2.*"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Kitbs\\EChineseLearning\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
